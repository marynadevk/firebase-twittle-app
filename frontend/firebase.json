{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/post/**",
        "destination": "post/[id]/index.html"
      },
      {
        "source": "/profile/**",
        "destination": "profile/[id]/index.html"
      }
    ]
  }
}
